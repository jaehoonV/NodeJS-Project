<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Log Page</title>
    <link rel="stylesheet" href="/stylesheets/logPage.css">
    <link rel="stylesheet" href="../node_modules/paginationjs/dist/pagination.css">
    <script type="text/javascript" src="../node_modules/jquery/dist/jquery.js"></script>
    <script type="text/javascript" src="../node_modules/paginationjs/dist/pagination.js"></script>
    <script type="text/javascript" src="/script/logPage.js" defer></script>
</head>
<body>
    <div style="margin: 0 auto; width: 80%;">
        <h1 style="margin: 10px auto; width: 150px;">Log Page</h1>
        
        <% if(master_yn){ %>
            <div id="log_history">
                <div id="log_data_container">
                    <div class="loader">
                        <span><i></i></span>
                    </div>
                </div>
                <div class="pagination-container" id="log_container_page"></div>
            </div>
            <div id="log_history">
                <div id="log_act_data_container">
                    <div class="loader">
                        <span><i></i></span>
                    </div>
                </div>
                <div class="pagination-container" id="log_act_container_page"></div>
            </div>
            <div id="log_history">
                <div id="log_move_data_container">
                    <div class="loader">
                        <span><i></i></span>
                    </div>
                </div>
                <div class="pagination-container" id="log_move_container_page"></div>
            </div>
        <% } %>
    </div>
</body>
</html>